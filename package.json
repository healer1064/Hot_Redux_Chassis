{
  "name": "hot-redux-chassis",
  "version": "0.0.1",
  "author": "Hanse",
  "license": "MIT",
  "engines": {
    "node": "7.2.1"
  },
  "scripts": {
    "postinstall": "./scripts/postinstall",
    "start": "nodemon --ignore app --ignore tests --exec babel-node -- server/index.js",
    "build": "NODE_ENV=production webpack  --config webpack/webpack.config.babel.js --color -p --progress --env",
    "build:dll": "webpack --display-chunks --color --config webpack/webpack.dll.babel.js",
    "flow": "flow",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint app --ignore-path .gitignore",
    "lint:css": "stylelint ./app/**/*.css",
    "test": "NODE_ENV=test jest",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Hanse/hot-redux-chassis.git"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^19.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "^0.28.1",
    "enzyme": "^2.7.0",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-prettier": "^1.7.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-prettier": "^2.0.1",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "flow-bin": "^0.46.0",
    "html-webpack-plugin": "^2.28.0",
    "identity-obj-proxy": "3.0.0",
    "imports-loader": "^0.7.0",
    "jest": "^19.0.2",
    "json-loader": "0.5.4",
    "nodemon": "1.11.0",
    "null-loader": "0.1.1",
    "postcss-cssnext": "^2.10.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.2.1",
    "postcss-nested": "^1.0.1",
    "prettier": "^1.3.1",
    "raw-loader": "0.5.1",
    "react-addons-test-utils": "^15.4.2",
    "style-loader": "^0.17.0",
    "stylelint": "^7.7.0",
    "stylelint-config-standard": "^16.0.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.5.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.20.0",
    "body-parser": "^1.16.1",
    "classnames": "2.2.5",
    "empty": "0.10.1",
    "express": "^4.14.1",
    "history": "^4.5.0",
    "immutable": "3.8.1",
    "immutable-devtools": "0.0.7",
    "isomorphic-fetch": "2.2.1",
    "json-server": "^0.10.0",
    "lodash": "^4.17.4",
    "node-uuid": "^1.4.8",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "v3.0.0-beta.6",
    "react-markdown": "^2.4.4",
    "react-overlays": "^0.7.0",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "3.6.0",
    "redux-logger": "^3.0.1",
    "redux-observable": "^0.14.1",
    "rxjs": "^5.3.1",
    "webpack-express-middleware": "^1.2.1"
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "string-quotes": "single",
      "indentation": 2,
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "global"
          ]
        }
      ]
    }
  },
  "jest": {
    "globals": {
      "__DEV__": false
    },
    "moduleDirectories": [
      "<rootDir>",
      "node_modules"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm)$": "<rootDir>/tests/FileStub.js",
      "^.+\\.css$": "<rootDir>/tests/CSSStub.js"
    },
    "testRegex": "\\.spec\\.(js|jsx)$"
  },
  "dllConfig": {
    "path": "dist",
    "exclude": [
      "express",
      "node-uuid",
      "json-server",
      "webpack-express-middleware"
    ]
  }
}
