{
  "name": "hot-redux-chassis",
  "version": "0.0.1",
  "author": "Hanse",
  "license": "MIT",
  "scripts": {
    "start": "nodemon --ignore app --ignore tests --exec babel-node -- server/index.js",
    "build": "NODE_ENV=production webpack  --config webpack/webpack.config.babel.js --color -p --progress --env",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint app --ignore-path .gitignore",
    "lint:css": "stylelint ./app/**/*.css",
    "test": "NODE_ENV=test karma start tests/karma.conf.js --single-run",
    "test:watch": "npm run test -- --auto-watch --no-single-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Hanse/hot-redux-chassis.git"
  },
  "dependencies": {
    "babel": "6.5.2",
    "babel-cli": "6.11.4",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.4",
    "babel-preset-es2015-native-modules": "6.9.2",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "body-parser": "1.15.2",
    "chai": "3.5.0",
    "chai-enzyme": "0.5.0",
    "chalk": "1.1.3",
    "classnames": "2.2.5",
    "css-loader": "0.23.1",
    "empty": "0.10.1",
    "enzyme": "2.4.1",
    "eslint": "3.1.1",
    "eslint-config-airbnb": "9.0.1",
    "eslint-import-resolver-webpack": "0.4.0",
    "eslint-plugin-flow-vars": "0.5.0",
    "eslint-plugin-import": "1.12.0",
    "eslint-plugin-jsx-a11y": "2.0.1",
    "eslint-plugin-react": "5.2.2",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "2.0.0-beta.3",
    "file-loader": "0.9.0",
    "history": "3.0.0",
    "html-webpack-plugin": "2.22.0",
    "immutable": "3.8.1",
    "immutable-devtools": "0.0.7",
    "imports-loader": "0.6.5",
    "jsdom": "9.4.1",
    "json-loader": "0.5.4",
    "json-server": "0.8.17",
    "karma": "1.1.2",
    "karma-chrome-launcher": "1.0.1",
    "karma-mocha": "1.1.1",
    "karma-mocha-reporter": "2.0.5",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.7.0",
    "lodash": "4.14.0",
    "mocha": "2.5.3",
    "node-uuid": "1.4.7",
    "nodemon": "1.10.0",
    "null-loader": "0.1.1",
    "postcss-cssnext": "2.7.0",
    "postcss-import": "8.1.2",
    "postcss-loader": "0.9.1",
    "postcss-nested": "1.0.0",
    "react": "15.2.1",
    "react-addons-test-utils": "15.2.1",
    "react-dom": "15.2.1",
    "react-hot-loader": "3.0.0-beta.1",
    "react-redux": "4.4.5",
    "react-router": "2.6.0",
    "react-router-redux": "4.0.5",
    "redux": "3.5.2",
    "redux-logger": "2.6.1",
    "redux-thunk": "^2.1.0",
    "sinon": "1.17.5",
    "style-loader": "0.13.1",
    "stylelint": "7.0.3",
    "stylelint-config-standard": "11.0.0",
    "url-loader": "0.5.7",
    "webpack": "2.1.0-beta.20",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.12.2"
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "string-quotes": "single",
      "indentation": 2
    }
  }
}
