{
  "name": "hot-redux-chassis",
  "version": "1.0.0",
  "author": "Hanse",
  "license": "MIT",
  "scripts": {
    "postinstall": "./scripts/postinstall",
    "start": "nodemon --ignore app --ignore tests --exec babel-node -- server/index.js",
    "build": "NODE_ENV=production webpack  --config webpack/webpack.config.babel.js --color -p --progress --env",
    "build:dll": "webpack --display-chunks --color --config webpack/webpack.dll.babel.js",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint app --ext ts,tsx,js --ignore-path .gitignore",
    "lint:css": "stylelint \"app/**/*.css\"",
    "test": "NODE_ENV=test jest",
    "test:watch": "npm run test -- --watch",
    "ci": "yarn test && yarn lint && yarn tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Hanse/hot-redux-chassis.git"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.3.4",
    "@babel/node": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.3.4",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-react-jsx-source": "7.2.0",
    "@babel/polyfill": "7.2.5",
    "@babel/preset-env": "7.3.4",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "@types/classnames": "2.2.7",
    "@types/history": "4.7.2",
    "@types/jest": "24.0.11",
    "@types/node": "11.11.3",
    "@types/react": "16.8.8",
    "@types/react-dom": "16.8.2",
    "@types/react-modal": "3.8.1",
    "@types/react-redux": "7.0.1",
    "@types/react-router": "4.4.5",
    "@types/react-router-dom": "4.3.1",
    "@types/react-test-renderer": "16.8.1",
    "@types/redux-logger": "3.0.7",
    "@typescript-eslint/eslint-plugin": "1.4.2",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.5.0",
    "babel-loader": "8.0.5",
    "babel-plugin-dynamic-import-node": "2.2.0",
    "css-loader": "2.1.1",
    "eslint": "5.15.2",
    "eslint-config-prettier": "4.1.0",
    "eslint-config-react-app": "3.0.8",
    "eslint-import-resolver-webpack": "0.11.0",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-react-hooks": "1.5.1",
    "file-loader": "3.0.1",
    "friendly-errors-webpack-plugin": "1.7.0",
    "html-webpack-plugin": "3.2.0",
    "identity-obj-proxy": "3.0.0",
    "imports-loader": "0.8.0",
    "jest": "24.5.0",
    "json-loader": "0.5.7",
    "mini-css-extract-plugin": "^0.5.0",
    "nodemon": "1.18.10",
    "null-loader": "0.1.1",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-nested": "4.1.2",
    "postcss-preset-env": "6.6.0",
    "prettier": "1.16.4",
    "raf": "3.4.1",
    "raw-loader": "1.0.0",
    "react-addons-test-utils": "15.6.2",
    "react-hot-loader": "4.8.0",
    "react-test-renderer": "16.8.4",
    "style-loader": "0.23.1",
    "stylelint": "9.10.1",
    "stylelint-config-standard": "18.2.0",
    "terser-webpack-plugin": "1.2.3",
    "typescript": "3.3.3333",
    "url-loader": "1.1.2",
    "webpack": "4.29.6",
    "webpack-cli": "3.3.0",
    "webpack-dev-middleware": "3.6.1",
    "webpack-hot-middleware": "2.24.3"
  },
  "dependencies": {
    "body-parser": "1.18.3",
    "classnames": "2.2.6",
    "connected-react-router": "6.3.2",
    "dom-scroll-into-view": "1.2.1",
    "express": "4.16.4",
    "fuzzysearch": "1.0.3",
    "history": "4.9.0",
    "isomorphic-fetch": "2.2.1",
    "node-uuid": "1.4.8",
    "query-string": "^6.4.0",
    "react": "16.8.4",
    "react-dom": "16.8.4",
    "react-markdown": "4.0.6",
    "react-modal": "3.8.1",
    "react-redux": "6.0.1",
    "react-router": "4.4.0",
    "react-router-dom": "4.4.0",
    "redux": "4.0.1",
    "redux-logger": "3.0.6",
    "redux-observable": "1.0.0",
    "rxjs": "6.4.0"
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "string-quotes": "single",
      "indentation": 2,
      "property-no-unknown": [
        true,
        {
          "ignoreProperties": [
            "composes"
          ]
        }
      ],
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "global"
          ]
        }
      ]
    }
  },
  "jest": {
    "globals": {
      "__DEV__": false
    },
    "moduleDirectories": [
      "<rootDir>",
      "node_modules"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm)$": "<rootDir>/tests/FileStub.js",
      "^.+\\.css$": "<rootDir>/tests/CSSStub.js"
    },
    "testRegex": "\\.spec\\.(js|jsx|ts|tsx)$",
    "setupFiles": [
      "raf/polyfill"
    ]
  },
  "dllConfig": {
    "path": "dist",
    "exclude": [
      "express",
      "node-uuid"
    ]
  }
}
