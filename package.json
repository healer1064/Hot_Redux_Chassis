{
  "name": "hot-redux-chassis",
  "version": "0.1.0",
  "author": "Hanse",
  "license": "MIT",
  "engines": {
    "node": "9.5.x"
  },
  "scripts": {
    "postinstall": "./scripts/postinstall",
    "start": "nodemon --ignore app --ignore tests --exec babel-node -- server/index.js",
    "build": "NODE_ENV=production webpack  --config webpack/webpack.config.babel.js --color -p --progress --env",
    "build:dll": "webpack --display-chunks --color --config webpack/webpack.dll.babel.js",
    "flow": "flow",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint app --ignore-path .gitignore",
    "lint:css": "stylelint \"app/**/*.css\"",
    "test": "NODE_ENV=test jest",
    "test:watch": "npm run test -- --watch",
    "ci": "yarn test && yarn lint && yarn flow"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Hanse/hot-redux-chassis.git"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.2.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-dynamic-import-node": "^1.0.2",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.17.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-plugin-flowtype": "^2.44.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "5.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.6.1",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.6",
    "flow-bin": "^0.66.0",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "3.0.0",
    "imports-loader": "^0.7.0",
    "jest": "^22.3.0",
    "json-loader": "^0.5.7",
    "nodemon": "^1.14.3",
    "null-loader": "0.1.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "postcss-nested": "^3.0.0",
    "prettier": "^1.9.2",
    "raf": "^3.4.0",
    "raw-loader": "0.5.1",
    "react-addons-test-utils": "^15.6.0",
    "style-loader": "^0.19.1",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^2.0.3",
    "webpack-hot-middleware": "^2.20.0"
  },
  "dependencies": {
    "add": "^2.0.6",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "body-parser": "^1.18.2",
    "classnames": "2.2.5",
    "dom-scroll-into-view": "^1.2.1",
    "express": "^4.16.2",
    "fuzzysearch": "^1.0.3",
    "history": "^4.7.2",
    "isomorphic-fetch": "2.2.1",
    "lodash": "^4.17.4",
    "node-uuid": "^1.4.8",
    "query-string": "^5.0.1",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-hot-loader": "^3.1.3",
    "react-loadable": "^5.3.1",
    "react-markdown": "^3.1.3",
    "react-modal": "^3.1.10",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "5.0.0-alpha.8",
    "react-test-renderer": "^16.1.1",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-observable": "^0.17.0",
    "rxjs": "^5.5.6"
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "string-quotes": "single",
      "indentation": 2,
      "property-no-unknown": [
        true,
        {
          "ignoreProperties": [
            "composes"
          ]
        }
      ],
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "global"
          ]
        }
      ]
    }
  },
  "jest": {
    "globals": {
      "__DEV__": false
    },
    "moduleDirectories": [
      "<rootDir>",
      "node_modules"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm)$": "<rootDir>/tests/FileStub.js",
      "^.+\\.css$": "<rootDir>/tests/CSSStub.js"
    },
    "testRegex": "\\.spec\\.(js|jsx)$",
    "setupFiles": [
      "raf/polyfill"
    ]
  },
  "eslintIgnore": [
    "flow-typed/npm"
  ],
  "dllConfig": {
    "path": "dist",
    "exclude": [
      "express",
      "node-uuid"
    ]
  }
}
