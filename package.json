{
  "name": "hot-redux-chassis",
  "version": "0.1.0",
  "author": "Hanse",
  "license": "MIT",
  "engines": {
    "node": "9.11.x"
  },
  "scripts": {
    "postinstall": "./scripts/postinstall",
    "start": "nodemon --ignore app --ignore tests --exec babel-node -- server/index.js",
    "build": "NODE_ENV=production webpack  --config webpack/webpack.config.babel.js --color -p --progress --env",
    "build:dll": "webpack --display-chunks --color --config webpack/webpack.dll.babel.js",
    "flow": "flow",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint app --ignore-path .gitignore",
    "lint:css": "stylelint \"app/**/*.css\"",
    "test": "NODE_ENV=test jest",
    "test:watch": "npm run test -- --watch",
    "ci": "yarn test && yarn lint && yarn flow"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Hanse/hot-redux-chassis.git"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "babel-jest": "23.0.1",
    "babel-loader": "7.1.4",
    "babel-plugin-dynamic-import-node": "1.2.0",
    "babel-plugin-transform-react-jsx-source": "6.22.0",
    "babel-preset-react": "6.24.1",
    "css-loader": "0.28.11",
    "eslint": "4.19.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-config-react-app": "2.1.0",
    "eslint-import-resolver-webpack": "0.10.0",
    "eslint-plugin-flowtype": "2.49.3",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-jsx-a11y": "5.0.1",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "7.9.1",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "1.1.11",
    "flow-bin": "0.74.0",
    "friendly-errors-webpack-plugin": "1.7.0",
    "html-webpack-plugin": "3.2.0",
    "identity-obj-proxy": "3.0.0",
    "imports-loader": "0.8.0",
    "jest": "23.1.0",
    "json-loader": "0.5.7",
    "nodemon": "1.17.5",
    "null-loader": "0.1.1",
    "postcss-cssnext": "3.1.0",
    "postcss-import": "11.1.0",
    "postcss-loader": "2.1.5",
    "postcss-nested": "3.0.0",
    "prettier": "1.13.5",
    "raf": "3.4.0",
    "raw-loader": "0.5.1",
    "react-addons-test-utils": "15.6.2",
    "style-loader": "0.21.0",
    "stylelint": "9.2.1",
    "stylelint-config-standard": "18.2.0",
    "url-loader": "1.0.1",
    "webpack": "4.12.0",
    "webpack-cli": "^3.0.4",
    "webpack-dev-middleware": "3.1.3",
    "webpack-hot-middleware": "2.22.2"
  },
  "dependencies": {
    "add": "2.0.6",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-stage-2": "6.24.1",
    "body-parser": "1.18.3",
    "classnames": "2.2.6",
    "dom-scroll-into-view": "1.2.1",
    "express": "4.16.3",
    "fuzzysearch": "1.0.3",
    "history": "4.7.2",
    "isomorphic-fetch": "2.2.1",
    "lodash": "4.17.10",
    "node-uuid": "1.4.8",
    "query-string": "6.1.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-hot-loader": "^4.3.2",
    "react-loadable": "5.4.0",
    "react-markdown": "3.3.2",
    "react-modal": "3.4.5",
    "react-redux": "5.0.7",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-router-redux": "5.0.0-alpha.8",
    "react-test-renderer": "^16.4.1",
    "redux": "4.0.0",
    "redux-logger": "3.0.6",
    "redux-observable": "0.19.0",
    "rxjs": "5.5.8"
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "string-quotes": "single",
      "indentation": 2,
      "property-no-unknown": [
        true,
        {
          "ignoreProperties": [
            "composes"
          ]
        }
      ],
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "global"
          ]
        }
      ]
    }
  },
  "jest": {
    "globals": {
      "__DEV__": false
    },
    "moduleDirectories": [
      "<rootDir>",
      "node_modules"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm)$": "<rootDir>/tests/FileStub.js",
      "^.+\\.css$": "<rootDir>/tests/CSSStub.js"
    },
    "testRegex": "\\.spec\\.(js|jsx)$",
    "setupFiles": [
      "raf/polyfill"
    ]
  },
  "eslintIgnore": [
    "flow-typed/npm"
  ],
  "dllConfig": {
    "path": "dist",
    "exclude": [
      "express",
      "node-uuid"
    ]
  }
}
